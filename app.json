{
  "expo": {
    "name": "MangaDise",
    "slug": "MangaDise",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/simbol.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/simbol.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kevinlidan.MangaDise"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/simbol.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/simbol.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "743435a7-cfe5-4d7d-a283-7f9580fd5c3e"
      }
    }
  }
}
